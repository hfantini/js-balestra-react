{
  "name": "js-balestra-react",
  "description": "Balestra engine support for react library.",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "Graphics",
    "Canvas",
    "Engine",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hfantini/js-balestra-react"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=src/lib/**/*.{js,jsx,ts,tsx}"
  },
  "dependencies": {
    "balestra": "^0.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.22.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.2",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.5.0",
    "react-test-renderer": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.63.6",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.1"
  },
  "files": [
    "dist",
    "README.MD"
  ],
  "main": "./dist/balestra-react.umd.js",
  "module": "./dist/balestra-react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/balestra-react.es.js",
      "require": "./dist/balestra-react.umd.js"
    },
    "./package.json": "./package.json"
  }
}
