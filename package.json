{
  "name": "js-balestra-react",
  "description": "Balestra engine support for react library.",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "Graphics",
    "Canvas",
    "Engine",
    "React"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hfantini/js-balestra-react"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "balestra": "^0.0.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.3.2",
    "@types/react": "^18.0.37",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "sass": "^1.63.6",
    "typescript": "^5.0.2",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.0.1"
  },
  "files": [
    "dist",
    "README.MD"
  ],
  "main": "./dist/balestra-react.umd.js",
  "module": "./dist/balestra-react.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/balestra-react.es.js",
      "require": "./dist/balestra-react.umd.js"
    },
    "./package.json": "./package.json"
  }
}
